{
    "name": "xtremepush/magento2-core-plugin",
    "description": "Xtremepush extension for Magento 2",
    "type": "magento2-module",
    "version": "1.0.0",
    "authors": [
        {
            "name": "Xtremepush",
            "homepage": "https://www.xtremepush.com/"
        }
    ],
    "license": [
        "proprietary"
    ],
    "require":
    {
        "php": ">=7.3",
        "magento/framework": ">=100.0.*",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Xtremepush\\Core\\": ""
        }
    },
    "config": {
        "optimize-autoloader": true
    }
}
